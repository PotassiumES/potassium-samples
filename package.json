{
  "name": "potassium-samples",
  "version": "0.1.0",
  "description": "Sample uses of PotassiumES",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && http-server .",
    "collect-static": "./scripts/collect-static.sh",
    "build-hello-world": "cross-env NODE_ENV=production rollup -c hello-world/rollup.config.js",
    "build-single-page-app": "cross-env NODE_ENV=production rollup -c single-page-app/rollup.config.js",
    "build": "npm run collect-static && npm run build-hello-world && npm run build-single-page-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PotassiumES/potassium-samples.git"
  },
  "keywords": [
    "webxr",
    "ar",
    "vr",
    "potassiumes",
    "javascript-framework"
  ],
  "author": "Trevor F. Smith <trevor@transmutable.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PotassiumES/potassium-samples/issues"
  },
  "homepage": "https://github.com/PotassiumES/potassium-samples#readme",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "rollup": "^0.62.0",
    "rollup-plugin-cleanup": "^3.0.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "dependencies": {
    "potassium-components": "git+https://github.com/PotassiumES/potassium-components.git",
    "potassium-es": "git+https://github.com/PotassiumES/potassium-es.git",
    "webxr-polyfill": "^1.0.6"
  }
}
