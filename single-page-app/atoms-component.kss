/* AtomsComponent */

.som.atoms-component > .media-grid-component {
	centroid-y: -7cm;
}

.som.atoms-component > .media-grid-component > .collection-component {
	display: grid;
	grid-template: 12cm / 20cm 20cm 20cm;
	grid-auto-rows: 12cm;
	gap: 1cm;
}

.som.atoms-component .video-component {
	scale: 3;
}
